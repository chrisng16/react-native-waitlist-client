    "eslint": "^8.57.0",
    "eslint-config-universe": "^12.0.1",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "react-native-dotenv": "^3.4.11",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.3.3"
