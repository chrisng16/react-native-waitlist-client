    "@rneui/themed": "^4.0.0-rc.8",
    "@supabase/supabase-js": "^2.48.1",
    "dayjs": "^1.11.13",
    "expo": "^52.0.37",
    "expo-navigation-bar": "~4.0.8",
    "expo-status-bar": "~2.0.1",
    "nativewind": "latest",
    "react": "18.3.1",
    "react-hook-form": "^7.54.2",
    "react-native": "0.76.7",
